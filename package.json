{
  "name": "upload-to-cloud-action",
  "version": "0.0.1",
  "private": true,
  "description": "Github action to upload file to cloud store",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "npm run lint -- --fix",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slice-varunnayal/upload-to-cloud-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup",
    "cloud upload",
    "s3",
    "aws"
  ],
  "author": "SlicePay",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.3",
    "aws-sdk": "^2.658.0",
    "klaw-sync": "^6.0.0",
    "lodash.set": "^4.3.2",
    "mime": "^2.4.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jest": "^25.2.1",
    "@types/lodash.set": "^4.3.6",
    "@types/mime": "^2.0.1",
    "@types/node": "^13.13.0",
    "@typescript-eslint/parser": "^2.28.0",
    "@zeit/ncc": "^0.22.1",
    "eslint": "^6.8.0",
    "eslint-plugin-github": "^3.4.1",
    "eslint-plugin-jest": "^23.8.2",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "jest-circus": "^25.4.0",
    "js-yaml": "^3.13.1",
    "lint-staged": "^10.1.6",
    "prettier": "^2.0.4",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3"
  }
}
